# npm configuration for Serial Debugger Frontend

# Prevent accidental publishing
publish = false

# Use strict peer dependency resolution
strict-peer-deps = true

# Enable package-lock.json
lockfile-version = 3

# Disable optional dependencies that might cause issues
optional = false

# Set default registry
registry = https://registry.npmjs.org/

# Prevent npm from installing devDependencies in production
production = false